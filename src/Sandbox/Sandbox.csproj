<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
     <TargetFramework>netcoreapp3.1</TargetFramework> 
    <PlatformTarget>AnyCPU</PlatformTarget>
    <LangVersion>8.0</LangVersion>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Nullable>enable</Nullable>
    <ApplicationIcon>Elffy.ico</ApplicationIcon>
    <Configurations>Debug;Release;Develop</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Elffy.Games\Elffy.Games.csproj" />
    <ProjectReference Include="..\Elffy\Elffy.csproj" />
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="&quot;$(ProjectDir)externalExe\erc.exe&quot; &quot;-r&quot; &quot;$(ProjectDir)ElffyResources&quot; &quot;-s&quot; &quot;$(ProjectDir)Scene&quot; &quot;$(ProjectDir)$(OutDir)\&quot;" />
  </Target>

</Project>
